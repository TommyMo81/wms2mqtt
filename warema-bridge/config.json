{
  "name": "Warema Bridge by TommyMo81",
  "version": "latest",
  "slug": "warema-bridge",
  "description": "Control your Warema WMS system",
  "arch": ["aarch64", "amd64", "armv7", "i386"],
  "startup": "services",
  "url": "https://github.com/TommyMo81/wms2mqtt",
  "boot": "auto",
  "hassio_api": true,
  "image": "tommymo81/wms2mqtt",
  "options": {
    "wms_serial_port": null
  },
  "schema": {
    "WMS_KEY": "str?",
    "WMS_PAN_ID": "str?",
    "WMS_CHANNEL": "int?",
    "WMS_SERIAL_PORT": "device(subsystem=tty)",
    "IGNORED_DEVICES": "str?",
    "FORCE_DEVICES": "str?"
  },
  "services": [
    "mqtt:need"
  ]
}
